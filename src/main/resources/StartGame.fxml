<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stickhero.SceneController">
    <children>
      <ImageView fitHeight="400.0" fitWidth="590.0" layoutX="-1.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Group 32StartGame-bg.png" />
         </image>
      </ImageView>
        <ImageView fitHeight="56.0" fitWidth="58.0" layoutX="275.0" layoutY="243.0" onMouseClicked="#switchToChooseCharacter" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/playButton.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="49.0" fitWidth="184.0" layoutX="224.0" layoutY="313.0" onMouseClicked="#switchToHowToPlayOne" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/howToPlayButton.png" />
            </image>
        </ImageView>
      <ImageView fx:id="FlyingHarry" fitHeight="216.0" fitWidth="184.0" layoutX="91.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/startgame-flyingHarry.png" />
         </image>
      </ImageView>
      <ImageView fx:id="SettingButton" fitHeight="41.0" fitWidth="36.0" layoutX="544.0" layoutY="4.0" onMouseClicked="#turnONsetting" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/StartGame-Setting%20Button.png" />
         </image>
      </ImageView>
      <ImageView fx:id="MusicON" fitHeight="41.0" fitWidth="36.0" layoutX="497.0" layoutY="5.0" onMouseClicked="#turnOFFmusic" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/StartGame-MusicStart.png" />
         </image>
      </ImageView>
      <ImageView fx:id="MusicOFF" fitHeight="41.0" fitWidth="36.0" layoutX="497.0" layoutY="5.0" onMouseClicked="#turnONmusic" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/StartGame-MusicStop.png" />
         </image>
      </ImageView>
      <ImageView fx:id="SettingBar" fitHeight="150.0" fitWidth="200.0" layoutX="382.0" layoutY="47.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/StartGame-VolumeSetting.png" />
         </image>
      </ImageView>
      <Slider fx:id="volumeSlider" blendMode="OVERLAY" layoutX="473.0" layoutY="75.0" prefHeight="14.0" prefWidth="109.0" />
      <ImageView fx:id="SettingButton2" fitHeight="41.0" fitWidth="36.0" layoutX="544.0" layoutY="4.0" onMouseClicked="#turnOFFsetting" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/StartGame-Setting%20Button.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
